{
  "env": {
    "browser": true,
    "es2021": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "script"
  },
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["warn", "single"],
    "indent": ["warn", 4]
  },
  "globals": {
    "slideIndex": "writable"
  }
}
